{
  "name": "telebot",
  "version": "2.0.0",
  "type": "module",
  "private": true,
  "description": "Simple script bot telegram using telegraf.",
  "imports": {
    "#/*": "./*",
    "#config/*": "./src/config/*.js",
    "#core/*": "./src/core/*.js",
    "#plugins/*": "./src/plugins/*.js",
    "#lib/*": "./src/lib/*.js",
    "#db/*": "./src/database/*.js"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "start": "node main.js",
    "dev": "node main.js",
    "lint": "eslint .",
    "format": "prettier -w ."
  },
  "dependencies": {
    "axios": "^1.7.7",
    "boxen": "^7.1.1",
    "chokidar": "^3.6.0",
    "dotenv": "^16.4.5",
    "telegraf": "^4.16.3"
  },
  "devDependencies": {
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3"
  }
}
